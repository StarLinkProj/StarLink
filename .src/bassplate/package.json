{
  "name": "bassplate",
  "version": "6.0.2",
  "description": "Boilerplate for Basscss with cssnext",
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "basscss": "^8.0.0",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-basic": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "cssnano": "^3.4.0",
    "postcss": "^5.0.14",
    "postcss-basscss": "^0.2.3",
    "postcss-calc": "^5.2.0",
    "postcss-cli": "^2.4.0",
    "postcss-color-function": "^2.0.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-discard-comments": "^2.0.3",
    "postcss-for": "^2.1.1",
    "postcss-import": "^8.0.2",
    "postcss-nesting": "^2.3.1",
    "postcss-plugin-context": "^2.0.0",
    "postcss-url": "^5.1.2",
    "postcss-use": "^2.3.0"
  },
  "scripts": {
    "basscss": "postcss -c postcss.config.js",
    "minify": "postcss -u cssnano css/base.css -o css/base.min.css",
    "start": "postcss -c postcss.config.js -w",
    "starlink": "postcss -c postcss-starlink.config.js",
    "debug": "postcss-debug src/debugstyles.css",
    "prettify": "cd .. & cd css & PrettyCSS base.css > bass.pretty.css --ignore-all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/basscss/bassplate.git"
  },
  "keywords": [
    "basscss",
    "boilerplate",
    "css",
    "oocss",
    "functional css",
    "postcss"
  ],
  "bugs": {
    "url": "https://github.com/basscss/bassplate/issues"
  },
  "homepage": "https://github.com/basscss/bassplate"
}
